<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.mv.hands">

    <application
        android:allowBackup="true"
        android:icon="@drawable/dininglogo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/handslogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MyOrderActivity"></activity>
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DiningListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".ShoppingCartActivity" />
        <activity android:name=".CommentWriteActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>